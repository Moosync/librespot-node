{
  "name": "librespot-node",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "cargo-cp-artifact -a cdylib librespot-node dist/build/librespot.node -- cargo build --manifest-path=native/Cargo.toml --message-format=json-render-diagnostics",
    "build-debug": "yarn run build",
    "build-release": "yarn run build -- --release",
    "install": "yarn run build-release",
    "webpack:build": "webpack --config webpack.config.js",
    "start": "yarn run install && yarn run webpack:build && node dist/index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/bindings": "^1.5.1",
    "@types/node": "^18.11.9",
    "bindings": "^1.5.0",
    "cargo-cp-artifact": "^0.1.7",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "devDependencies": {}
}